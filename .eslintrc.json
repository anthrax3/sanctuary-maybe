{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es3.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "plugins": ["markdown"],
      "env": {"es6": true},
      "globals": {"Just": false, "Maybe": false, "Nothing": false, "Z": false, "show": false},
      "rules": {
        "no-extra-semi": ["off"],
        "strict": ["off"]
      }
    }
  ]
}
